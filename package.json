{
  "name": "tsc-ticket",
  "version": "1.0.0",
  "private": true,
  "main": "electron/main.js",
  "scripts": {
    "dev": "ts-node --project tsconfig.server.json server.ts",
    "dev:next": "next dev",
    "build": "prisma generate && next build",
    "start": "NODE_ENV=production ts-node --project tsconfig.server.json server.ts",
    "lint": "eslint",
    "db:push": "prisma db push",
    "db:seed": "npx ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts",
    "db:studio": "prisma studio",
    "postinstall": "prisma generate",
    "dns": "node scripts/dns-server.js",
    "electron:dev": "concurrently \"npm run dev\" \"wait-on http://localhost:3000 && electron .\"",
    "electron:build": "electron-builder --win --x64",
    "electron:pack": "electron-builder --dir"
  },
  "prisma": {
    "seed": "npx ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@hookform/resolvers": "^5.2.2",
    "@prisma/client": "^5.22.0",
    "@types/mailparser": "^3.4.6",
    "@types/speakeasy": "^2.0.10",
    "@types/uuid": "^10.0.0",
    "bcryptjs": "^3.0.3",
    "date-fns": "^4.1.0",
    "imap-simple": "^5.1.0",
    "lucide-react": "^0.563.0",
    "mailparser": "^3.9.3",
    "next": "16.1.4",
    "next-auth": "^4.24.13",
    "nodemailer": "^7.0.12",
    "prisma": "^5.22.0",
    "qrcode": "^1.5.4",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "react-hook-form": "^7.71.1",
    "recharts": "^3.7.0",
    "socket.io": "^4.8.3",
    "socket.io-client": "^4.8.3",
    "sonner": "^2.0.7",
    "speakeasy": "^2.0.0",
    "uuid": "^13.0.0",
    "zod": "^4.3.6",
    "zustand": "^5.0.10"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/bcryptjs": "^2.4.6",
    "@types/node": "^20",
    "@types/nodemailer": "^7.0.5",
    "@types/qrcode": "^1.5.6",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "concurrently": "^9.2.1",
    "dns2": "^2.1.0",
    "electron": "^40.1.0",
    "electron-builder": "^26.7.0",
    "eslint": "^9",
    "eslint-config-next": "16.1.4",
    "tailwindcss": "^4",
    "ts-node": "^10.9.2",
    "typescript": "^5",
    "wait-on": "^9.0.3"
  }
}
